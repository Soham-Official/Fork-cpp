void isPrime(int n)
{
    
    if(n==1)
    cout<<"No"<<endl;
    else
    {
        int flag=0;
        for(int i=2;i<=sqrt(n);i++)
         {
        if(n%i==0)
        {
            cout<<"No"<<endl;
            flag=1;
            break;
            
        }
          }
       if(flag==0)
       cout<<"Yes"<<endl;
       
    }
    
}
