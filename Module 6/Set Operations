https://practice.geeksforgeeks.org/problems/set-operations/1/?track=module-6-sets-and-maps&batchId=113


set<int> setInsert(int arr[],int n)
{
    set<int>s;
    
    for(int i=0;i<n;i++)
    s.insert(arr[i]);
    
    return s;
    
}


void setDisplay(set<int>s1)
{
    for(auto itr = s1.begin(); itr != s1.end(); itr++ )
    cout<<*itr<<" ";
    cout<<endl;
}
