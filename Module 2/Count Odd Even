https://practice.geeksforgeeks.org/problems/count-odd-even/1/?track=module-2-arrays-and-strings&batchId=113


void countOddEven(int a[], int n)
    {
        int even=0;
        int odd=0;
        for(int i=0;i<n;i++)
        {
            if(a[i]%2==0)
            even++;
            else
            odd++;
        }
        
        cout<<odd<<" "<<even<<endl;
    }
