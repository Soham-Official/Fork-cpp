void swap(int *A, int *B){
    
    int c;
    c=*A;
    *A=*B;
    *B=c;
    
}

// Function to swap A and B
// using reference
void swap(int &A, int &B){
  
    int z;
    z=A;
    A=B;
    B=z;
  
}
