int area(Rectangle r)
{
   return r.length*r.breadth;
}


void maxArea(Rectangle arr[],int n)
{
   
   int maxarea=0;
   for(int i=0;i<n;i++)
   maxarea=max(maxarea,area(arr[i]));
   cout<<maxarea;
    cout<<endl;
}
